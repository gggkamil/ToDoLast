@model ToDoLast.Models.Job

@{
    ViewData["Title"] = "Details";
}

<h1>Details</h1>

<div>
    <h4>Job</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            <label class="control-label">Task name</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.JobQuestion)
        </dd>
        <dt class = "col-sm-2">
            <label class="control-label">Description</label>
        </dt>
        <dd class = "col-sm-10">
            @Html.DisplayFor(model => model.JobAnswer)
        </dd>
    </dl>
</div>
<div>
    <a asp-action="Edit" type="button" class="btn btn-primary" asp-route-id="@Model?.Id">Edit</a>
    <a type="button" class="btn btn-primary" asp-action="Index">Back to List</a>
</div>
